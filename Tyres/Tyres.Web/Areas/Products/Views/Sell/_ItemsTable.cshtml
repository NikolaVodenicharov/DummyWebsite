@model IEnumerable<ItemDTO>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>Product</th>
            <th>Price</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ProductName</td>
                <td>@item.Price.ToMoney()</td>
                <td>@item.Quantity</td>
            </tr>
        }
    </tbody>
</table>